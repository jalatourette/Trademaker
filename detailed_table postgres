create table secondary_stock_data (
  ticker varchar(5) primary key foreign key,
  52_wk_low ,
  52_wk_high ,
  volume ,
  avg_volume ,
  market_cap ,
  dividend ,
  beta ,
  expected_earnings_date ,
  peg_ratio ,
  zacks_rank ,
  industry varchar(256),
  sector varchar(256),
  fiscal_year_end ,
  last_reported_quarter ,
  next_eps_date date,
  price_yesterday_close numeric(30,15),
  target_price_consensus numeric(30,15),
  dividend_yield numeric(30,15),
  pe_f1 ,
  eps_growth_vs_prev_year ,
  eps_growth_vs_prev_quarter ,
  sales_growth_vs_prev_year ,
  sales_growth_vs_prev_quarter ,
  price_book numeric(30,15),
  price_cash_flow numeric(30,15),
  price_sales numeric(30,15),
  next_earnings_date date,
  earnings_esp ,
  pe ,
  sales_yoy_growth_current_quarter numeric(30,15),
    --must be decimal
  sales_yoy_growth_next_quarter numeric(30,15),
    --must be decimal
  earnings_yoy_growth_current_quarter numeric(30,15),
    --must be decimal
  earnings_yoy_growth_next_quarter numeric(30,15),
    --must be decimal
  surprise_earnings numeric(30,15),
  reported_earnings ,
  current_abr ,
  number_of_recs_in_abr numeric(30,15),
  avg_target_price ,
  net_profit_margin ,
  return_on_equity ,
  debt_to_equity
  ) ;
